{"remainingRequest":"D:\\JAVA\\WK\\HospitalManager\\HospitalManagerVue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\JAVA\\WK\\HospitalManager\\HospitalManagerVue\\src\\views\\OrderList.vue?vue&type=script&lang=js&","dependencies":[{"path":"D:\\JAVA\\WK\\HospitalManager\\HospitalManagerVue\\src\\views\\OrderList.vue","mtime":1728354671651},{"path":"D:\\JAVA\\WK\\HospitalManager\\HospitalManagerVue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1689001367240},{"path":"D:\\JAVA\\WK\\HospitalManager\\HospitalManagerVue\\node_modules\\babel-loader\\lib\\index.js","mtime":1689001368654},{"path":"D:\\JAVA\\WK\\HospitalManager\\HospitalManagerVue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1689001367240},{"path":"D:\\JAVA\\WK\\HospitalManager\\HospitalManagerVue\\node_modules\\vue-loader\\lib\\index.js","mtime":1689001369023}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["OrderList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0HA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"OrderList.vue","sourceRoot":"src/views","sourcesContent":["<template>\n    <!-- 卡片 -->\n    <el-card>\n        <!-- 搜索栏 -->\n        <el-row type=\"flex\">\n            <el-col :span=\"6\">\n                <el-input v-model=\"query\" placeholder=\"Search_by_id\">\n                    <el-button\n                        slot=\"append\"\n                        style=\"font-size: 18px;\"\n                        @click=\"requestOrders\"\n                    ><i class=\"iconfont icon-r-find\" style=\"font-size: 22px;\"></i> search</el-button>\n                </el-input>\n            </el-col>\n        </el-row>\n        <!-- 表格 -->\n        <el-table :data=\"orderData\" stripe style=\"width: 100%\" border>\n            <el-table-column\n                prop=\"oId\"\n                label=\"NO\"\n                width=\"80px\"\n            ></el-table-column>\n            <el-table-column\n                prop=\"pId\"\n                label=\"patient_id\"\n                width=\"80px\"\n            ></el-table-column>\n\n            <el-table-column prop=\"dId\" label=\"doctor_id\" width=\"100px\">\n            </el-table-column>\n\n            <el-table-column\n                prop=\"oStart\"\n                label=\"regist_time\"\n                width=\"180px\"\n            ></el-table-column>\n            <el-table-column\n                prop=\"oEnd\"\n                label=\"End_time\"\n                width=\"180px\"\n            ></el-table-column>\n            <el-table-column\n                prop=\"oRecord\"\n                label=\"reason\"\n                width=\"400px\"\n            ></el-table-column>\n            <el-table-column\n                prop=\"oDrug\"\n                label=\"drug\"\n                width=\"180px\"\n            ></el-table-column>\n            <el-table-column\n                prop=\"oCheck\"\n                label=\"check_item\"\n                width=\"180px\"\n            ></el-table-column>\n            <el-table-column\n                prop=\"oTotalPrice\"\n                label=\"fee/Yuan\"\n                width=\"80px\"\n            ></el-table-column>\n            <el-table-column prop=\"oPriceState\" label=\"payment_state\" width=\"100px\">\n                <template slot-scope=\"scope\">\n                    <el-tag type=\"success\" v-if=\"scope.row.oPriceState === 1\"\n                        >payed</el-tag\n                    >\n                    <!-- <el-tag type=\"danger\" v-if=\"scope.row.oPriceState === 0 && scope.row.oState === 1\">unpay</el-tag> -->\n                    <el-button\n                        type=\"danger\"\n                        size=\"mini\"\n                        v-if=\"\n                            scope.row.oPriceState === 0 &&\n                            scope.row.oState === 1\n                        \"\n                        @click=\"priceClick(scope.row.oId)\"\n                        >PAY</el-button\n                    >\n                </template>\n            </el-table-column>\n            <el-table-column prop=\"oState\" label=\"Regist_state\" width=\"100px\">\n                <template slot-scope=\"scope\">\n                    <el-tag\n                        type=\"success\"\n                        v-if=\"\n                            scope.row.oState === 1 &&\n                            scope.row.oPriceState === 1\n                        \"\n                        >done</el-tag\n                    >\n                    <el-tag\n                        type=\"danger\"\n                        v-if=\"scope.row.oState === 0 && scope.row.oState === 0\"\n                        >undone</el-tag\n                    >\n                </template>\n            </el-table-column>\n            <el-table-column label=\"Operation\" width=\"140\" fixed=\"right\">\n                <template slot-scope=\"scope\">\n                    <el-button\n                        style=\"font-size: 18px\"\n                        type=\"danger\"\n                        @click=\"deleteDialog(scope.row.oId)\"\n                    ><i class=\"iconfont icon-r-delete\" style=\"font-size: 22px;\"></i> delete</el-button>\n                </template>\n            </el-table-column>\n        </el-table>\n\n        <!-- 分页 -->\n        <el-pagination\n            @size-change=\"handleSizeChange\"\n            @current-change=\"handleCurrentChange\"\n            background\n            layout=\"total, sizes, prev, pager, next, jumper\"\n            :current-page=\"pageNumber\"\n            :page-size=\"size\"\n            :page-sizes=\"[1, 2, 4, 8, 16]\"\n            :total=\"total\"\n        >\n        </el-pagination>\n    </el-card>\n</template>\n<script>\nimport request from \"@/utils/request.js\";\nimport { toLoad } from \"@/utils/initialize.js\";\nexport default {\n    name: \"OrderList\",\n    data() {\n        return {\n            pageNumber: 1,\n            size: 8,\n            query: \"\",\n            orderData: [],\n            total: 3,\n        };\n    },\n    methods: {\n        //删除挂号操作\n        deleteOrder(id) {\n            request\n                .get(\"admin/deleteOrder\", {\n                    params: {\n                        oId: id,\n                    },\n                })\n                .then((res) => {\n                    this.requestOrders();\n                    console.log(res);\n                });\n        },\n        //删除对话框\n        deleteDialog(id) {\n            this.$confirm(\"delete?\", \"Tip\", {\n                confirmButtonText: \"Yes\",\n                cancelButtonText: \"No\",\n                type: \"warning\",\n            })\n                .then(() => {\n                    this.deleteOrder(id);\n                    this.$message({\n                        type: \"success\",\n                        message: \"success!\",\n                    });\n                })\n                .catch(() => {\n                    this.$message({\n                        type: \"info\",\n                        message: \"canceled\",\n                    });\n                });\n        },\n        //页面大小改变时触发\n        handleSizeChange(size) {\n            this.size = size;\n            this.requestOrders();\n        },\n        //   页码改变时触发\n        handleCurrentChange(num) {\n            console.log(num);\n            this.pageNumber = num;\n            this.requestOrders();\n        },\n        // 加载订单列表\n        requestOrders() {\n            request\n                .get(\"admin/findAllOrders\", {\n                    params: {\n                        pageNumber: this.pageNumber,\n                        size: this.size,\n                        query: this.query,\n                    },\n                })\n                .then((res) => {\n                    this.orderData = res.data.data.orders;\n                    this.total = res.data.data.total;\n                    toLoad()\n                });\n        },\n    },\n    created() {\n        this.requestOrders();\n    },\n};\n</script>\n<style scoped lang=\"scss\">\n.el-table {\n    margin-top: 20px;\n    margin-bottom: 20px;\n}\n.el-form {\n    margin-top: 0;\n}\n</style>"]}]}